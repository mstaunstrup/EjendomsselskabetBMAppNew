<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.marianne.ejendomsselskabetbmapp" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:parentActivityName=".Forside" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.marianne.ejendomsselskabetbmapp.Forside" />
        </activity>
        <activity
            android:name=".Forside"
            android:label="@string/title_activity_forside" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TaskListe"
            android:label="@string/title_activity_task_liste"
            android:parentActivityName=".Forside" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.marianne.ejendomsselskabetbmapp.Forside" />
        </activity>
        <activity
            android:name=".SingleTask"
            android:label="@string/title_activity_single_task" >
        </activity>
        <activity
            android:name=".NewTaskList"
            android:label="@string/title_activity_new_task_list"
            android:parentActivityName=".Forside"
            android:theme="@style/AppTheme.NoActionBar" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.marianne.ejendomsselskabetbmapp.Forside" />
        </activity>
    </application>

</manifest>
